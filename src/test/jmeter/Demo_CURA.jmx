              <stringProp name="script">// Giả sử biến redirectLocation đã được khai báo trước đó
String redirectLocation = &quot;https://katalon-demo-cura.herokuapp.com/#appointment&quot;;

// Kiểm tra và lấy phần sau dấu #
String redirectPath = &quot;&quot;;
int hashIndex = redirectLocation.indexOf(&quot;#&quot;);
if (hashIndex != -1) {
    redirectPath = redirectLocation.substring(hashIndex);
}

// Lưu vào biến JMeter để Debug Sampler có thể hiển thị
vars.put(&quot;redirectPath&quot;, redirectPath);

// In ra log để kiểm tra
log.info(&quot;Redirect Path: &quot; + redirectPath);
</stringProp>
